package module4;

public class Lis {
	public void count(int[] arr) {
		int[] count = new int[arr.length];
		for (int i = 0; i < arr.length; i++) {
			int max = arr[i];
			for (int j = i+1; j < arr.length; j++) {
				if (max < arr[j]) {
					max = arr[i];
					count[i]++;
				}
			}

		}
		int m =count[0];
		for(int k =1;k<count.length;k++) {
			if(count[k]>m)
				m=count[k];
		}
		System.out.println("lis = "+ m);
	}
	public static void main(String[] args) {
		Lis r = new Lis();
		int arr[] = { 3,10,2,1,20 };
		r.count(arr);
	}

}
