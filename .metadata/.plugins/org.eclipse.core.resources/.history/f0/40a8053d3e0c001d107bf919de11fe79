package module4;

public class Lis {
	public void count(int[] arr) {
		int count =1,lis=0;
		for (int i = 0; i < arr.length; i++) {
			int max = arr[i];
			for (int j = i+1; j < arr.length; j++) {
				if ( arr[j] > max ) {
					max = arr[j];
					count++;
					System.out.println(count);
				}
				if(count>lis)
					lis=count;
			}
		}
		System.out.println("lis = "+ lis);
	}
	public static void main(String[] args) {
		Lis r = new Lis();
		int arr[] = { 50, 3, 10, 7, 40, 80 };
		r.count(arr);
	}

}
